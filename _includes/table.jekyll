<div class="w3-responsive">
  <table class="w3-table-all w3-hoverable w3-card-2">
    {% for item in {{include.data}} %}
      <tr>
        {% if forloop.first %}
          {% for col in item %}
            <th class="w3-theme">{{col[1]}}</th>
          {% endfor %}
        {% else %}  	
          {% for col in item %}
            <td>{{col[1]}}</td>
          {% endfor %}
        {% endif %}  	
      </tr>
    {% endfor %}
  </table>
</div>